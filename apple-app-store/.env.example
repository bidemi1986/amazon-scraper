# Apple App Store Configuration
# Copy this file to .env and replace with your actual app details

# The numeric App Store ID (found at the end of the URL)
# Example: https://apps.apple.com/us/app/whatsapp-messenger/id310633997
# APP_ID would be: 310633997
APP_ID=310633997

# Countries to fetch reviews from (comma-separated, optional)
# Default is 'us' if not specified
# Common country codes: us, gb, ca, au, de, fr, jp, cn, in, br
COUNTRIES=us,gb,ca

# Examples for other apps:
# WhatsApp: APP_ID=310633997
# Instagram: APP_ID=389801252
# TikTok: APP_ID=835599320
# Spotify: APP_ID=324684580
# Netflix: APP_ID=363590051
